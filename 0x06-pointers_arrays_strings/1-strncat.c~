#include "holberton.h"
#include <string.h>
/**
 **_strncat - creates a new string by combining two
 *
 *@dest: string destination
 *
 *@src:  string source
 *
 *@n: number of characters to append
 *
 *Return: 0 = Success
 */
char *_strncat(char *dest, char *src, int n)
{

char *d[10];
char *s[10];
if (n != 0)
n++;
{
if (d != '\0')
return (dest);
}
{
if (s != '\0')
return (src);
}
{
return (0);
}
}
